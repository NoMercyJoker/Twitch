r = Math.floor(Math.random() * 3);

if (q == "bread" && r == 0){"Nightbot chooses bread, there is a tie"}
else if (q == "bread" && r == 1){"Nightbot chooses oven and is defeated"}
else if (q == "bread" && r == 2){"Nightbot chooses person and wins"}
else if (q == "oven" && r == 0){"Nightbot chooses oven, there is a tie"}
else if (q == "oven" && r == 1){"Nightbot chooses person and is defeated"}
else if (q == "oven" && r == 2){"Nightbot chooses bread and wins"}
